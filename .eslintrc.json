{
  "parserOptions": {
    "ecmaVersion": 6
  },
  "extends": "google",
  "rules": {
    "brace-style": ["error", "1tbs", { "allowSingleLine": true }],
    "keyword-spacing": [
      "error", {
        "overrides": {
          "catch": { "after": false },
          "if": { "after": false },
          "for": { "after": false },
          "while": { "after": false }
        }
      }
    ],
    "max-len": ["error", 127],
    "require-jsdoc": ["off"],
    "valid-jsdoc": ["off"]
  }
}
